{
  "packages": {
    ".": {
      "release-type": "simple",
      "prerelease": true,
      "prerelease-type": "rc",
      "changelog-path": "CHANGELOG.md"
    }
  },
  "extra-files": [
    {
      "type": "regex",
      "path": "build.gradle",
      "pattern": "version = '(?<version>.*)'"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}